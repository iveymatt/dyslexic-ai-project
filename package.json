{
  "name": "dyslexic-ai-benchmark",
  "version": "2.0.0",
  "description": "Production-ready benchmark system for evaluating AI tools through a neurodivergent-first lens",
  "main": "index.js",
  "scripts": {
    "demo": "tsx benchmark/demo.ts",
    "demo:visual": "tsx benchmark/demo-visual.ts",
    "suite": "tsx benchmark/cli/runSuite.ts",
    "suite:run": "tsx benchmark/cli/runSuite.ts run",
    "suite:compare": "tsx benchmark/cli/runSuite.ts compare",
    "suite:estimate": "tsx benchmark/cli/runSuite.ts estimate",
    "suite:list": "tsx benchmark/cli/runSuite.ts list-suites",
    "models:list": "tsx benchmark/cli/runSuite.ts list-models",
    "benchmark": "tsx benchmark/cli/benchmark.ts",
    "benchmark:run": "tsx benchmark/cli/benchmark.ts run",
    "benchmark:validate": "tsx benchmark/cli/benchmark.ts validate",
    "benchmark:dimensions": "tsx benchmark/cli/benchmark.ts dimensions",
    "benchmark:tests": "tsx benchmark/cli/benchmark.ts tests",
    "benchmark:checks": "tsx benchmark/cli/benchmark.ts checks",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "dev": "tsx watch benchmark/cli/benchmark.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [
    "benchmark",
    "accessibility",
    "neurodivergent",
    "dyslexia",
    "ai",
    "llm"
  ],
  "author": "Dyslexic AI Project",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prisma": "^5.22.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
